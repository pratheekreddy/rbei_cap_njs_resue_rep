{
	"_version": "1.12.0",
	"sap.app": {
		"id": "RBEI_UI5.rbei_ui5_reuse_rep",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"settingModel": {
				"uri": "model/settingModel.json",
				"type": "JSON"
			},
			"repo": {
				"uri": "/srv_test/repo/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml",
					"odataVersion": "4.0"
				}
			},
			"master": {
				"uri": "/srv_test/master/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml",
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "RBEI_UI5.rbei_ui5_reuse_rep.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.f": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "RBEI_UI5.rbei_ui5_reuse_rep.i18n.i18n"
				}
			},
			"settingModel": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/settingModel.json"
			},
			"navigation": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "model/Navigation.json"
			},
			"odata_model": {
				"type": "sap.ui.model.odata.v4.ODataModel",
				"settings": {
					"operationMode": "Server",
					"synchronizationMode": "None",
					"groupId": "$direct"
				},
				"dataSource": "repo",
				"preload": false
			},
			"odata_model2": {
				"type": "sap.ui.model.odata.v4.ODataModel",
				"settings": {
					"operationMode": "Server",
					"synchronizationMode": "None",
					"groupId": "$direct"
				},
				"dataSource": "master",
				"preload": false
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "RBEI_UI5.rbei_ui5_reuse_rep.view",
				"controlId": "flexibleColumnLayout",
				"controlAggregation": "pages",
				"clearControlAggregation": false,
				"viewLevel": 1
			},
			"routes": [{
				"name": "ToolPage",
				"pattern": ":layout:",
				"target": ["TargetView1",
				"ObjectsDetailsPage"]
			}, {
				"name": "View2",
				"pattern": "RouteView1",
				"target": ["View2"]
			}, {
				"pattern": "ObjectsDetailsPage/{product}/{layout}",
				"name": "ObjectsDetailsPage",
				"target": ["TargetView1",
				"ObjectsDetailsPage"]
			}],
			"targets": {
				"TargetView1": {
					"viewName": "ToolPage",
					"controlAggregation": "beginColumnPages"
				},
				"View2": {
					"viewType": "XML",
					"viewName": "View2"
				},
				"ObjectsDetailsPage": {
					"viewName": "ObjectsDetailsPage",
					"controlAggregation": "midColumnPages"
				}
			}
		}
	}
}